<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/user_online" />
            <stroke android:width="1dp" android:color="@color/user_online" />

        </shape>

    </item>
    <item>
        <shape android:shape="oval" >
            <stroke android:width="1dp" android:color="@color/colorError" />
        </shape>
    </item>
</selector>